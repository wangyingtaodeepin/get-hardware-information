{
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "bootloadertype": "ipxe",
                "kernel": "http://lava.deepin.io/tftpboot/vmlinuz",
                "nfsrootfs": "http://lava.deepin.io/tftpboot/deepin.tar.gz",
                "ramdisk": "http://lava.deepin.io/tftpboot/initrd.img",
                "target_type": "deepin"
            }
        },
        {
            "command": "boot_linaro_image"
        },
        {
            "command": "lava_test_shell",
            "parameters": {
                "testdef_repos": [
                    {
                        "git-repo": "https://github.com/wangyingtaodeepin/get-hardware-information.git",
                        "testdef": "get-hardware-information.yaml"
                    }
                ],
                "timeout": 900
            }
        },
        {
            "command": "submit_results",
            "parameters": {
                "server": "http://lava.deepin.io/RPC2/",
                "stream": "/anonymous/network/"
            }
        }
    ],
    "device_type": "x86_skip_ipxe",
    "job_name": "get-hardware-information",
    "timeout": 18000
}
